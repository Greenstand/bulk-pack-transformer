{
  "name": "treetracker",
  "version": "1.4.7",
  "private": true,
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0 <17",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "test": "npm run test-unit && npm run test-int",
    "test-unit": "NODE_ENV=test mocha -r dotenv/config --env dotenv_config_path=.env.test --exit ./src/**/*.spec.js",
    "test-int": "NODE_ENV=test mocha -r dotenv/config --env dotenv_config_path=.env.test --exit --timeout 20000 './index.spec.js'",
    "server": "node index.js",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "prepare": "husky install",
    "pre-commit": "npm test && npm run eslint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.1.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "express-async-errors": "^3.1.1",
    "express-bearer-token": "^2.1.1",
    "jsonwebtoken": "8.1.0",
    "morgan": "^1.9.1",
    "pg": "^8.7.0",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "uuid": "^3.3.2",
    "uuid-by-string": "^3.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "chai": "^4.0.0",
    "database-cleaner": "^1.3.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "mocha": "^8.1.3",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.7.0",
    "supertest": "^4.0.2"
  }
}
